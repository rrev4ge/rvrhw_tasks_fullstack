{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        // "plugin:jsx-a11y/recommended",
        "plugin:react/recommended",
        "semistandard"
    ],
    // "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-shadow": "off",
    "react/state-in-constructor": [2, "never"]
    }
}